"####################
"number
"####################
set number
set numberwidth=5
set hlsearch
set backspace=indent,eol,start
setl expandtab tabstop=4 shiftwidth=4 softtabstop=4
set fileencodings=iso-2022-jp,cp932,sjis,euc-jp,utf-8

set ignorecase
set smartcase

" swp files
set directory=~/.vim/swp

" listchar
set listchars=eol:~,tab:>.
